<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:dubbo="http://dubbo.apache.org/schema/dubbo"
    xsi:schemaLocation="http://www.springframework.org/schema/beans        http://www.springframework.org/schema/beans/spring-beans-4.3.xsd        http://dubbo.apache.org/schema/dubbo        http://dubbo.apache.org/schema/dubbo/dubbo.xsd">
 
 	<!-- 1、指定当前服务/应用的名字（同样的服务名字相同，不要和别的服务同名） -->
 	<dubbo:application name="user-service-provider"></dubbo:application>
 	
 	<!-- 2、指定指定注册中心的位置 -->
 	<!-- <dubbo:registry address="zookeeper://127.0.0.1:2181"></dubbo:registry> -->
 	<dubbo:registry protocol="zookeeper" address="127.0.0.1:2181"></dubbo:registry>
 	
 	<!-- 3、指定通信规则（通信协议？通信端口） -->
 	<dubbo:protocol name="dubbo" port="20880"></dubbo:protocol>
 	
 	<!-- 4、暴露服务 ref:执行服务的真正实现 -->
 	<dubbo:service interface="service.UserService" ref="userServiceImpl01"
 		timeout="1000" version="1.0.0">
 		<!-- <dubbo:method name="getUserAddressList" timeout="1000"></dubbo:method>	 -->
 	</dubbo:service>
 	<dubbo:service interface="service.UserService" ref="userServiceImpl02"
 		timeout="1000" version="2.0.0">
 		<!-- <dubbo:method name="getUserAddressList" timeout="1000"></dubbo:method>	 -->
 	</dubbo:service>
 	
 	<!-- 统一设置提供者设置 -->
 	<dubbo:provider timeout="6000"></dubbo:provider>
 	
 	<!-- 服务的实现 -->
 	<bean id="userServiceImpl01" class="service.impl.UserServiceImpl"></bean>
 	<bean id="userServiceImpl02" class="service.impl.UserServiceImpl2"></bean>
 	
 	
 	<!-- 链接监控中心 -->
 	<!-- 自动查找 -->
 	<dubbo:monitor protocol="registry"></dubbo:monitor>
 	<!-- 指定 -->
 	<!-- <dubbo:monitor address="127.0.0.1:7070"></dubbo:monitor> -->
 	
</beans>